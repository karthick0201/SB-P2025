package com.util;

import java.util.Arrays;
import java.util.List;

import com.binding.Customer;

public class CustomerUtils {
	
	public static List<Customer> getCustomerList(){
		
		Customer c1 = new Customer(101,"Arun","Prime",54);
		Customer c2 = new Customer(102,"Rafi","Prime Plus",78);
		Customer c3 = new Customer(103,"Dharan","Classic",44);
		Customer c4 = new Customer(104,"Akalya","Prime",96);
		Customer c5 = new Customer(105,"Kannan","Classic",12);
				
				
		return Arrays.asList(c1,c2,c3,c4,c5);
	}

}
