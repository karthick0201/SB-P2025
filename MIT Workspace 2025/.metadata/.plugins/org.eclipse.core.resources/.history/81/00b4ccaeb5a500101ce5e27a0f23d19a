package com.mit.entity;

import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.Id;
import jakarta.persistence.Table;

@Entity
@Table(name = "labtop")
public class Labtop {
	
	@Id
	@GeneratedValue
	private int id;
	
	private String name;
	
	private int ram;
	
	private int rom;

	
	public Labtop(String name, int ram, int rom) {
		super();
		this.name = name;
		this.ram = ram;
		this.rom = rom;
	}
	public Labtop() {
		// TODO Auto-generated constructor stub
	}
	@Override
	public String toString() {
		return id + " : " + name + " : " + ram + " : " + rom ;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getRam() {
		return ram;
	}

	

	public void setRam(int ram) {
		this.ram = ram;
	}

	public int getRom() {
		return rom;
	}

	public void setRom(int rom) {
		this.rom = rom;
	}

	
}
