package com.request;

import lombok.AllArgsConstructor;
import lombok.Builder;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@NoArgsConstructor
@AllArgsConstructor
@Builder
public class CustomerRequest {
	
	private String name;

	private String gender;

	private String email;

	private String phoneNo;

	private String location;
}
