package com.mit.entity.sequence;

import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.GenerationType;
import jakarta.persistence.Id;
import jakarta.persistence.SequenceGenerator;
import jakarta.persistence.Table;

@Entity
@Table(name = "eclipse_sequence_pratice")
public class Eclipse {

	@Id
	@GeneratedValue(strategy = GenerationType.SEQUENCE, generator = "sequence_generator_pratice")
	@SequenceGenerator(name = "sequence_generator_pratice", sequenceName = "eclipse_seq", initialValue = 100, allocationSize = 2)
	private int id;

	private String name;

	private String color;

}
