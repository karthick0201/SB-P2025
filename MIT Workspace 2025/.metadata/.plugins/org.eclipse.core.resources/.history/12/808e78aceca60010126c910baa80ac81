@SpringBootApplication
public class Sb09DatajpaMongodbApplication implements CommandLineRunner{

	public static void main(String[] args) {
		SpringApplication.run(Sb09DatajpaMongodbApplication.class, args);
	}
	
	
	@Autowired
	private StudentDao studentDao;

	@Override
	public void run(String... args) throws Exception {
		
		System.out.println(">>----------| Pratcing MongoDB in JPA");
		
		Student s = new Student();
		
		s.setName("Karthick R");
		String[] ids = {"k@123.com","p@123.com"};
		s.setEmailIds({"k@123.com","p@123.com"});
		
		
		
		
		
		
	}

}
